<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>$(NoWarn);CA1711</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyTitle>AttributeReader</AssemblyTitle>
    <Description>Sample project for Zentient.Metadata attribute reading.</Description>
    <Company>Zentient Framework Team</Company>
    <Authors>Zentient Framework Team</Authors>
    <Copyright>Copyright Â© 2025 Zentient Framework Team. All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/ulfbou/Zentient.Metadata.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ulfbou/Zentient.Metadata.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\src\Zentient.Metadata.Attributes\Zentient.Metadata.Attributes.csproj" />
  </ItemGroup>

</Project>
