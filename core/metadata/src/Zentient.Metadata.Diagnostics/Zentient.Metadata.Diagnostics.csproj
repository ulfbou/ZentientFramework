<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <AssemblyName>Zentient.Metadata.Diagnostics</AssemblyName>
    <RootNamespace>Zentient.Metadata.Diagnostics</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>Zentient.Metadata.Diagnostics</PackageId>
    <Authors>Zentient Framework Team</Authors>
    <Company>Zentient Framework Team</Company>
    <Description>Metadata-driven diagnostics, profiles, and DX helpers for Zentient.Metadata</Description>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Zentient.Metadata\Zentient.Metadata.csproj" />
    <PackageReference Include="Zentient.Abstractions" Version="3.0.2" />
  </ItemGroup>
</Project>
