{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "properties": {
        "TargetFramework": "net8.0"
      },
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "README.md",
          "CHANGELOG.md",
          "CONTRIBUTING.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "LICENSE",
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      },
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "LIBRARY_NAME Documentation",
      "_appName": "LIBRARY_NAME",
      "_appFooter": "Copyright Â© 2025 Zentient Frameworks Team. All rights reserved. All rights reserved",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableAffix": false,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/ulfbou/Zentient.Analyzers.git",
        "branch": "main"
      },
      "_gitUrlPattern": "github",
      "_customMetadata": {
        "company": "Zentient Frameworks Team",
        "version": "1.0.0",
        "framework": "net8.0"
      }
    },
    "fileMetadata": {
      "_tocTitle": {
        "api/**.yml": "API Reference"
      },
      "_tocRel": {
        "api/**.md": "../toc.yml"
      }
    },
    "dest": "_site",
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "abbreviations",
        "autoidentifiers",
        "citations",
        "customcontainers",
        "definitionlists",
        "emphasisextras",
        "figures",
        "footers",
        "footnotes",
        "gridtables",
        "mathematics",
        "medialinks",
        "pipetables",
        "listextras",
        "tasklists",
        "diagrams",
        "autolinks",
        "attributes"
      ]
    }
  },
  "serve": {
    "port": 8080
  }
}
